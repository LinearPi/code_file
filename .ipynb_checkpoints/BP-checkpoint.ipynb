    "导数常见有哪些：\n",
    "\n",
    "y=c(c为常数) y'=0\n",
    "y=x^n y'=nx^(n-1)\n",
    "y=a^x y'=a^xlna\n",
    "y=e^x y'=e^x\n",
    "y=logax y'=logae/x\n",
    "y=lnx y'=1/x\n",
    "y=sinx y'=cosx\n",
    "y=cosx y'=-sinx\n",
    "y=tanx y'=1/cos^2x\n",
    "y=cotx y'=-1/sin^2x\n",
    "y=arcsinx y'=1/√1-x^2\n",
    "y=arccosx y'=-1/√1-x^2\n",
    "y=arctanx y'=1/1+x^2\n",
    "y=arccotx y'=-1/1+x^2\n"
   ]
  }
